{
    "name": "mern-2-ui",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "engines": {
        "node": "10.x",
        "npm": "6.x"
    },
    "scripts": {
        "#compile": "Generates JS bundles for production. Use with start.",
        "compile": "webpack --mode production",
        "compile-w": "webpack",
        "#start": "UI server, HMR is enabled in dev mode",
        "start": "node  dist/server.js",
        "#watch": "Compile, and recompile on any changes.",
        "##watch": "webpack --watch",
        "#watch-server-hmr": "Recompile server HRM bundle on changes.",
        "watch-server-hmr": "webpack -w --config webpack.serverHMR.js",
        "#dev-all": "Dev mode: watch for server changes and start UI server",
        "dev-all": "rm dist/* && npm run watch-server-hmr & sleep 5 && npm start",
        "mongo-start": "sudo service mongod start",
        "mongo-stop": "sudo service mongod stop",
        "#lint": "Runs ESLint on all relevant files",
        "lint": "eslint server src browser --ext js,jsx",
        "heroku-postbuild": "npm run compile && ln -sf ../node_modules/bootstrap/dist public/bootstrap"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^3.4.1",
        "dotenv": "^6.2.0",
        "express": "^4.17.1",
        "http-proxy-middleware": "^0.19.1",
        "isomorphic-fetch": "^2.2.1",
        "nodemon": "^1.19.1",
        "prop-types": "^15.7.2",
        "react": "^16.9.0",
        "react-bootstrap": "^0.32.4",
        "react-dom": "^16.9.0",
        "react-router-bootstrap": "^0.25.0",
        "react-router-dom": "^4.3.1",
        "react-select": "^2.4.4",
        "serialize-javascript": "^1.9.1",
        "source-map-support": "^0.5.13",
        "url-search-params": "^1.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.6",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.14.3",
        "webpack": "^4.39.1",
        "webpack-cli": "^3.3.6",
        "webpack-dev-middleware": "^3.7.0",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}